{
  "repos": {
    "idRepo" : {
      "type" : "index",
      "require" : true
    },
    "title": {
      "type"    : "string",
      "require" : true,
      "length"  : 100
    },
    "iconPath": {
      "type"  : "string",
      "length": 100
    },
    "url": {
      "type"  : "string",
      "require" : true,
      "length": 255
    },
    "status": {
      "require" : true,
      "type"    : "option",
        "option" : {
          "0" : "active",
          "1" : "trash",
          "2" : "block"
        }
    },
    "visibily" : {
      "require" : true,
      "type"    : "option",
        "option" : {
          "0" : "public",
          "1" : "private"
        }
    },
    "forker" : {
      "type"      : "index",
      "relation"  : "users.idUser"
    },
    "created" : {
      "type" : "date",
      "require" : true
    },
    "lastUpdated" : {
      "type" : "date",
      "require" : true
    },
    "ownership" : {
      "type" : "index",
      "relation" : "users.idUser"
    }
  }
}